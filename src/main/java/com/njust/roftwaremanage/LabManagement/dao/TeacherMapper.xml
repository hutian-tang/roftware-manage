<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="main.java.com.njust.roftwaremanage.LabManagement.dao.TableMapper">
    <!-- 添加Emp数据,parameterType -->
    <select id="findTablebyid" parameterType="String" resultType="main.java.com.njust.roftwaremanage.LabManagement.entity.Table">
        select * from table where student_id=#{student_id}
    </select>
   <select id="findTableByStudentIdAndArrangeId" parameterType="main.java.com.njust.roftwaremanage.LabManagement.entity.Table" resultType="main.java.com.njust.roftwaremanage.LabManagement.entity.Table">
        select * from table where student_id=#{student_id} and arrange_id=#{arrange_id}
    </select>
<select id="findTableByTableIdAndArrangeId" parameterType="main.java.com.njust.roftwaremanage.LabManagement.entity.Table" resultType="main.java.com.njust.roftwaremanage.LabManagement.entity.Table">
        select * from table where table_id=#{table_id} and arrange_id=#{arrange_id}
    </select>
<insert id="InsertTable" parameterType="main.java.com.njust.roftwaremanage.LabManagement.entity.Table">
	insert into (table_id,address,condition,arrange_id,student_id)values(#{table_id},#{address},#{condition},#{arrange_id},#{student_id})
</insert>
</mapper>
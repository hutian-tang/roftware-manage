<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>LoginController</servlet-name>
        <servlet-class>com.njust.roftwaremanage.LabManagement.controller.LoginController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginController</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ViewExperiment</servlet-name>
        <servlet-class>com.njust.roftwaremanage.LabManagement.controller.StudentViewExperiment</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ViewExperiment</servlet-name>
        <url-pattern>/viewExp</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ViewExperimentDetail</servlet-name>
        <servlet-class>com.njust.roftwaremanage.LabManagement.controller.StudentViewExperimentDetail</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ViewExperimentDetail</servlet-name>
        <url-pattern>/expDetail</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>StudentShowExp</servlet-name>
        <servlet-class>com.njust.roftwaremanage.LabManagement.controller.StudentShowExperiment</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>StudentShowExp</servlet-name>
        <url-pattern>/showExp</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>StudentSelectExp</servlet-name>
        <servlet-class>com.njust.roftwaremanage.LabManagement.controller.StudentSelectExperiment</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>StudentSelectExp</servlet-name>
        <url-pattern>/StudentTest/selectExp</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>StudentCancelExp</servlet-name>
        <servlet-class>com.njust.roftwaremanage.LabManagement.controller.StudentCancelExperiment</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>StudentCancelExp</servlet-name>
        <url-pattern>/StudentTest/cancelExp</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>StudentFeedBack</servlet-name>
        <servlet-class>com.njust.roftwaremanage.LabManagement.controller.StudentFeedbackExperiment</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>StudentFeedBack</servlet-name>
        <url-pattern>/StudentTest/feedback</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>StudentReceiveMag</servlet-name>
        <servlet-class>com.njust.roftwaremanage.LabManagement.controller.StudentReceiveMessageController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>StudentReceiveMag</servlet-name>
        <url-pattern>/studentMag</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>TeacherArrangeExperiment</servlet-name>
        <servlet-class>com.njust.roftwaremanage.LabManagement.controller.TeacherArrangeExperiment</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TeacherArrangeExperiment</servlet-name>
        <url-pattern>/TeacherArrangeExperiment</url-pattern>
    </servlet-mapping>
</web-app>
<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>LoginController</servlet-name>
        <servlet-class>com.njust.roftwaremanage.LabManagement.controller.LoginController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginController</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>TeacherArrangeExperiment</servlet-name>
        <servlet-class>com.njust.roftwaremanage.LabManagement.controller.TeacherArrangeExperiment</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TeacherArrangeExperiment</servlet-name>
        <url-pattern>/TeacherArrangeExperiment</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ViewExperiment</servlet-name>
        <servlet-class>com.njust.roftwaremanage.LabManagement.controller.StudentViewExperiment</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ViewExperiment</servlet-name>
        <url-pattern>/StudentHome</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ViewExperimentDetail</servlet-name>
        <servlet-class>com.njust.roftwaremanage.LabManagement.controller.StudentViewExperimentDetail</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ViewExperimentDetail</servlet-name>
        <url-pattern></url-pattern>
    </servlet-mapping>
</web-app>